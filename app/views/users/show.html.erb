<center>
<h1>Welcome, <%= @user.username %></h1>

<h3>My Spent Points: <%= @user.spent_points %></h3>
<h3>My Unspent Points: <%= @user.total_points %></h3>

<h3>My Rewards:</h3>
  <% @user.user_rewards.each do |reward| %>
  <p><%= reward.name %>
    <% end %>
    <%= link_to "Buy Rewards", new_user_reward_path(@user) %>
</center>
